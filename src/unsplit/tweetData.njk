---
    pagination:
        data: tweets
        size: 1000
    permalink: "splitTemplates/tweetData{{ pagination.pageNumber }}.11ty.js"
---
class TweetData {
    data() {
        return {
            pagination: {
                data: "bigdata{{ pagination.pageNumber }}",
                size: 1,
                alias: "tweet"
            },
            permalink: data => `realDonaldTrump/status/${data.tweet.id}/data.json`
        }
    }

    render(data) {
        return `${JSON.stringify(data.tweet)}`
    }
}

module.exports = TweetData;